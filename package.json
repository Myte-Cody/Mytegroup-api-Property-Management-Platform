{
  "name": "mytegroup-api-property-management-platform",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start:prod": "node dist/main.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "db:seed": "ts-node -r tsconfig-paths/register src/cli.ts seed:all",
    "lease:renew": "ts-node -r tsconfig-paths/register src/scripts/lease-renewal.ts process",
    "lease:renew:preview": "ts-node -r tsconfig-paths/register src/scripts/lease-renewal.ts preview",
    "lease:renew:live": "ts-node -r tsconfig-paths/register src/scripts/lease-renewal.ts process --no-dry-run",
    "status:update": "ts-node -r tsconfig-paths/register src/scripts/status-updater.ts"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@casl/mongoose": "^8.0.3",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "commander": "^13.1.0",
    "mongoose": "^8.17.1",
    "mongoose-delete": "^1.0.2",
    "nest-commander": "^3.19.0",
    "nestjs-form-data": "^1.9.92",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.5",
    "mongodb-memory-server": "^10.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
